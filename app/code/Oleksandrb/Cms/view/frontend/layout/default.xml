<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd"
>
    <body>
        <referenceContainer name="page.wrapper">
            <block class="Magento\Cms\Block\BlockByIdentifier"
                   name="page.demo.start.block"
                    before="-">

                <arguments>
                    <argument name="identifier" xsi:type="string">demo_start_block</argument>
                </arguments>
            </block>
        </referenceContainer>

        <referenceContainer name="oleksandrb.cms.sidebar.demo.container">
            <block class="Magento\Cms\Block\BlockByIdentifier"
                   name="sidebar_block_demo"
                   after="-">

                <arguments>
                    <argument name="identifier" xsi:type="string">sidebar_block_demo</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
